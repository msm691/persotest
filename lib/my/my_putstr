** putstr implementation
*/
#include <unistd.h>
#include "my.h"

int my_putstr(const char *s)
{
    int i;

    if (s == 0)
        s = "(null)";
    i = 0;
    while (s[i] != '\0') {
        write(1, &s[i], 1);
        i += 1;
    }
    return i;
}
